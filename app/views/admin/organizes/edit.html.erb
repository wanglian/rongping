<% content_for :header do -%>
  编辑客户帐号
<% end -%>

<% form_for(@organize, :url => admin_organize_path(@organize)) do |f| %>
  <%= f.error_messages %>

  <p><label for="name">名称</label><br/>
  <%= f.text_field :name %></p>

  <p><label for="contact">联系信息</label><br/>
  <%= f.text_area :contact %></p>
  
  <p>
    <%= f.submit "Update"[] %>
  </p>
<% end %>

<%= link_to 'Show'[], admin_organize_path(@organize) %> |
<%= link_to 'Back'[], admin_organizes_path %>
